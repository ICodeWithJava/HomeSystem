        </dependency>
        <dependency>
            <groupId>io.papermc.paperweight</groupId>
            <artifactId>paperweight-userdev</artifactId>
            <version>1.3.6</version>
        </dependency>
        <dependency>
            <groupId>com.zaxxer</groupId>
            <artifactId>HikariCP</artifactId>
            <version>6.3.0</version>
        </dependency>
